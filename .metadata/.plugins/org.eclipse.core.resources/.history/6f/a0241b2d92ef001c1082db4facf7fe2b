package com.Spring.ExampleAop;

import org.springframework.stereotype.Component;

public class ShoppingCart {
	
	/*
	 * add these following feature into the aspect 
	 * 1: logging 
	 * 2: Authentication andAuthorization
	 * 3: Data sanitization
	 */

	@Component
	public void checkOut() {
		System.out.println("Checkout method from the Shopping Cart");

	}
	
	
}
